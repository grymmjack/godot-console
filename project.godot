; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="godot-console"
config/description="Godot text mode console compatible with ANSI colors, iCE colors, and 8x16px DOS font for rendering ANSI art in Godot using tilemaps! "
config/version="0.1"
config/tags=PackedStringArray("git")
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://splash.png"
boot_splash/fullsize=false
config/icon="res://icon.svg"

[display]

window/size/viewport_width=640
window/size/viewport_height=480
window/subwindows/embed_subwindows=false
window/stretch/scale_mode="integer"
window/handheld/orientation=1

[rendering]

textures/canvas_textures/default_texture_filter=0
